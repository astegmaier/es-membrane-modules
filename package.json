{
  "name": "@microsoft-loop/es-membrane",
  "version": "0.0.3",
  "description": "A fork of es-membrane that uses modules and provides typescript definitions.",
  "license": "ISC",
  "author": "Andrew Stegmaier",
  "contributors": [
    {
      "name": "Alexander J. Vincent",
      "email": "ajvincent@gmail.com"
    },
    {
      "name": "Andrew Stegmaier",
      "email": "andrew.stegmaier@microsoft.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astegmaier/es-membrane-modules.git"
  },
  "bugs": {
    "url": "https://github.com/astegmaier/es-membrane-modules/issues"
  },
  "homepage": "https://github.com/astegmaier/es-membrane-modules#readme",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc && npm run copy:dts:cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm && npm run copy:dts:esm",
    "copy:dts:cjs": "copyfiles -u 1 \"src/**/*.d.ts\" dist/cjs",
    "copy:dts:esm": "copyfiles -u 1 \"src/**/*.d.ts\" dist/esm",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "assert": "^2.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.6.13",
    "@swc/jest": "^0.2.36",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  }
}
