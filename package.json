{
  "name": "@microsoft-loop/es-membrane",
  "version": "0.0.8",
  "description": "A fork of es-membrane that uses modules and provides typescript definitions.",
  "license": "ISC",
  "author": "Andrew Stegmaier",
  "contributors": [
    {
      "name": "Alexander J. Vincent",
      "email": "ajvincent@gmail.com"
    },
    {
      "name": "Andrew Stegmaier",
      "email": "andrew.stegmaier@microsoft.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astegmaier/es-membrane-modules.git"
  },
  "bugs": {
    "url": "https://github.com/astegmaier/es-membrane-modules/issues"
  },
  "homepage": "https://github.com/astegmaier/es-membrane-modules#readme",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "test": "node --expose-gc ./node_modules/jest/bin/jest.js",
    "typecheck": "npm run typecheck:src && npm run typecheck:test && npm run typecheck:mocks",
    "typecheck:test": "tsc --project test/tsconfig.json",
    "typecheck:src": "tsc --project src/tsconfig.json --noEmit",
    "typecheck:mocks": "tsc --project mocks/tsconfig.json",
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project src/tsconfig.json && npm run copy:dts:cjs",
    "build:esm": "tsc --project src/tsconfig.json --module es6 --outDir dist/esm && npm run copy:dts:esm",
    "copy:dts:cjs": "copyfiles -u 1 \"src/**/*.d.ts\" dist/cjs",
    "copy:dts:esm": "copyfiles -u 1 \"src/**/*.d.ts\" dist/esm",
    "clean": "rimraf dist failing-test-heapsnapshots",
    "format": "prettier --write src/**/*"
  },
  "dependencies": {},
  "devDependencies": {
    "@swc/core": "^1.6.13",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  }
}
